  <translation>
   <name>Remove passable liftgates</name>
   <description>Remove passable liftgates</description>
   <match mode="and">
     <tag k="barrier" v="lift_gate"/>
     <tag k="access" v="yes"/>
   </match>
   <output>
     <copy-unmatched/>
      <tag k="access" v="yes"/>
   </output>
 </translation>

