<translation>
   <name>Ferry transform</name>
   <description>Convert route=ferry+ferry=* to highway=*+ferry=yes </description>
   <match mode="and">
     <tag k="route" v="ferry"/>
     <tag k="ferry" v=".*" match_id="ferry_type"/>
   </match>
   <output>
     <copy-unmatched/>
     <tag k="ferry" v="yes"/>
     <tag k="highway" from_match="ferry_type"/>
   </output>
 </translation>
 
 <translation>
   <name>default levels for garages</name>
   <description>1 level is set for garages by default</description>
   <match mode="and">
     <tag k="building" v="garages"/>
     <notag k="building:levels" v=".*"/>
   </match>
   <output>
     <copy-all/>
     <tag k="building:levels" v="1"/>
   </output>
 </translation>
 
 <translation>
   <name>addr_place</name>
   <description>addr:place to addr:street</description>
   <match mode="and">
     <tag k="addr:place" v=".*" match_id="addr_place" />
     <notag k="addr:street" v=".*"/>
   </match>
   <output>
     <copy-unmatched/>
     <tag k="addr:street" from_match="addr_place" />
   </output>
 </translation>
 
</translations>
